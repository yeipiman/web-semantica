<http://data.barcelona.cat/actividades/actividad/688f26b5c763922b3f7ae123> a <http://data.barcelona.cat/actividades/Actividad> ;
    ns2:Aforo "487" ;
    ns2:Inscripcion "atenciociutadana@vic.cat" ;
