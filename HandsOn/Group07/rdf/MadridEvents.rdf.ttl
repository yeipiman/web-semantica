  mm:eventType "/contenido/actividades/CineActividadesAudiovisuales";
  mm:freeEvent true;
  mm:startsAtTime "19:00";
  mm:startsOnDate "2025-10-17T00:00:00Z"^^xsd:dateTime .

<http://movemadrid.org/resource/event/12761862> a mm:Event;
