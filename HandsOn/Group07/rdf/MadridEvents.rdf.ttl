  mm:eventTitle "Octubre Negro en Madrid: 'Hacia un nuevo orden mundial'";
  mm:eventType "/contenido/actividades/RecitalesPresentacionesActosLiterarios";
  mm:freeEvent true;
  mm:startsAtTime "18:40";
  mm:startsOnDate "2025-10-02T00:00:00Z"^^xsd:dateTime .

