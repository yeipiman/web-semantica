  mm:eventTitle "El Casting y Amateurs";
  mm:eventType "/contenido/actividades/TeatroPerformance";
  mm:freeEvent true;
  mm:startsAtTime "18:15";
  mm:startsOnDate "2025-10-03T00:00:00Z"^^xsd:dateTime .

