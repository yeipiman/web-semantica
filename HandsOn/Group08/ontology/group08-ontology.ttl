@prefix : <http://www.semanticweb.org/group08/ontology#> .
@prefix ex: <http://example.org/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .


# ONTOLOGY METADATA

<http://www.semanticweb.org/group08/ontology>
    rdf:type owl:Ontology ;
    rdfs:label "Smart City Ontology – Group08"@en ;
    rdfs:comment "Ontology for integrating Air Quality, Traffic, and Bike Parking data in Barcelona."@en .


# CLASSES

:Observation rdf:type owl:Class ;
  rdfs:label "Observation"@en ;
  rdfs:comment "General class for any observed phenomenon."@en .

:AirQualityObservation rdf:type owl:Class ;
  rdfs:subClassOf :Observation ;
  rdfs:label "Air Quality Observation"@en ;
  rdfs:comment "An observation of air pollutant concentration at a station and time."@en .

:TrafficSection rdf:type owl:Class ;
  rdfs:subClassOf :Observation ;
  rdfs:label "Traffic Section"@en ;
  rdfs:comment "Represents a monitored road section in the city traffic system."@en .

:BikeParking rdf:type owl:Class ;
  rdfs:label "Bike Parking"@en ;
  rdfs:comment "Represents a public or private parking facility for bicycles."@en .

:Location rdf:type owl:Class ;
  rdfs:label "Location"@en ;
  rdfs:comment "Geographical or administrative place with coordinates and name."@en .


# OBJECT PROPERTIES

:locatedIn rdf:type owl:ObjectProperty ;
  rdfs:domain :Observation ;
  rdfs:range :Location ;
  rdfs:label "located in"@en ;
  rdfs:comment "Links an observation or entity to its geographical location."@en .


# DATA PROPERTIES

### Generic identification and description
:hasID rdf:type owl:DatatypeProperty ;
  rdfs:domain :Observation ;
  rdfs:range xsd:string ;
  rdfs:label "has ID"@en .

:hasName rdf:type owl:DatatypeProperty ;
  rdfs:domain :Observation ;
  rdfs:range rdfs:Literal ;
  rdfs:label "has name"@en .

### Air Quality
:hasContaminantCode rdf:type owl:DatatypeProperty ;
  rdfs:domain :AirQualityObservation ;
  rdfs:range xsd:integer ;
  rdfs:label "has contaminant code"@en .

:hasContaminantValue rdf:type owl:DatatypeProperty ;
  rdfs:domain :AirQualityObservation ;
  rdfs:range xsd:decimal ;
  rdfs:label "has contaminant value"@en .

:hasObservationDateTime rdf:type owl:DatatypeProperty ;
  rdfs:domain :Observation ;
  rdfs:range xsd:dateTime ;
  rdfs:label "has observation date and time"@en .

:hasValidationFlag rdf:type owl:DatatypeProperty ;
  rdfs:domain :AirQualityObservation ;
  rdfs:range xsd:boolean ;
  rdfs:label "has validation flag"@en .

### Traffic
:hasCurrentTravelTime rdf:type owl:DatatypeProperty ;
  rdfs:domain :TrafficSection ;
  rdfs:range xsd:integer ;
  rdfs:label "has current travel time (seconds)"@en .

:hasPredictedTravelTime rdf:type owl:DatatypeProperty ;
  rdfs:domain :TrafficSection ;
  rdfs:range xsd:integer ;
  rdfs:label "has predicted travel time (seconds)"@en .

:hasCongestionFactor rdf:type owl:DatatypeProperty ;
  rdfs:domain :TrafficSection ;
  rdfs:range xsd:decimal ;
  rdfs:label "has congestion factor"@en .

:hasTrafficState rdf:type owl:DatatypeProperty ;
  rdfs:domain :TrafficSection ;
  rdfs:range rdfs:Literal ;
  rdfs:label "has traffic state"@en .

:hasTrafficTendency rdf:type owl:DatatypeProperty ;
  rdfs:domain :TrafficSection ;
  rdfs:range rdfs:Literal ;
  rdfs:label "has traffic tendency"@en .

### Bike Parking
:hasStreetNumber rdf:type owl:DatatypeProperty ;
  rdfs:domain :BikeParking ;
  rdfs:range xsd:integer ;
  rdfs:label "has street number"@en .

:hasZipCode rdf:type owl:DatatypeProperty ;
  rdfs:domain :BikeParking ;
  rdfs:range xsd:integer ;
  rdfs:label "has zip code"@en .

:hasContactType rdf:type owl:DatatypeProperty ;
  rdfs:domain :BikeParking ;
  rdfs:range rdfs:Literal ;
  rdfs:label "has contact type"@en .

:hasContactValue rdf:type owl:DatatypeProperty ;
  rdfs:domain :BikeParking ;
  rdfs:range rdfs:Literal ;
  rdfs:label "has contact value"@en .

### Location info
:hasMunicipality rdf:type owl:DatatypeProperty ;
  rdfs:domain :Location ;
  rdfs:range rdfs:Literal ;
  rdfs:label "has municipality"@en .

:hasDistrict rdf:type owl:DatatypeProperty ;
  rdfs:domain :Location ;
  rdfs:range rdfs:Literal ;
  rdfs:label "has district name"@en .

geo:lat rdf:type owl:DatatypeProperty ;
  rdfs:domain :Location ;
  rdfs:range xsd:decimal ;
  rdfs:label "latitude"@en .

geo:long rdf:type owl:DatatypeProperty ;
  rdfs:domain :Location ;
  rdfs:range xsd:decimal ;
  rdfs:label "longitude"@en .


# SAMPLE INDIVIDUALS (examples)

# Air Quality
ex:airObservation_Station4_2025-08-01T10:00:00 rdf:type :AirQualityObservation ;
  :hasID "AQ_4_20250801T10" ;
  :hasContaminantCode 7 ;
  :hasContaminantValue 21.4 ;
  :hasValidationFlag true ;
  :hasObservationDateTime "2025-08-01T10:00:00"^^xsd:dateTime ;
  :locatedIn ex:Barcelona .

# Traffic
ex:trafficSection_1_2025-08-01T10:00:00 rdf:type :TrafficSection ;
  :hasID "T_1_20250801T10" ;
  :hasCurrentTravelTime 302 ;
  :hasPredictedTravelTime 300 ;
  :hasCongestionFactor 1.01 ;
  :hasTrafficState "normal" ;
  :hasTrafficTendency "stable" ;
  :hasObservationDateTime "2025-08-01T10:00:00"^^xsd:dateTime ;
  :locatedIn ex:Barcelona .

# Bike Parking
ex:bikeParking_93308083222 rdf:type :BikeParking ;
  :hasName "Aparcament Estació Barcelona Nord" ;
  :hasStreetNumber 54 ;
  :hasZipCode 8013 ;
  :hasContactType "Telèfon" ;
  :hasContactValue "932658164" ;
  :locatedIn ex:Barcelona .

# Location (shared)
ex:Barcelona rdf:type :Location ;
  :hasMunicipality "Barcelona" ;
  :hasDistrict "Eixample" ;
  geo:lat 41.3943 ;
  geo:long 2.18198 .

