# ================================================
# SUPPORTING INSTANCES
# ================================================

## Transport Family
metroRes:family/metro a metro:TransportFamily ;
    rdfs:label "Metro" ;
    metro:familyOrder 1 .

## Operator
metroRes:operator/tmb a metro:Operator ;
    rdfs:label "Metro" ;
    rdfs:comment "Transports Metropolitans de Barcelona" .

## Accessibility Types
metroRes:accessibility/accessible a metro:AccessibilityType ;
    rdfs:label "Accessible" .

metroRes:accessibility/notAccessible a metro:AccessibilityType ;
    rdfs:label "Not Accessible" .

## Status Types
metroRes:status/operational a metro:StationStatus ;
    rdfs:label "Operatiu" .

metroRes:status/underConstruction a metro:StationStatus ;
    rdfs:label "Under Construction" .

metroRes:status/closed a metro:StationStatus ;
    rdfs:label "Closed" .

# ================================================
# LINE-STATION RELATIONSHIPS
# ================================================

## L1 Line relationships
metroRes:metroLine/L1 metro:hasStation metroRes:station/hospitalDeBellvitge ;
    metro:hasStation metroRes:station/bellvitge ;
    metro:hasStation metroRes:station/avCarrilet ;
    metro:hasStationLineRelation metroRes:stationLine/hospitalDeBellvitge_L1 ;
    metro:hasStationLineRelation metroRes:stationLine/bellvitge_L1 ;
    metro:hasStationLineRelation metroRes:stationLine/avCarrilet_L1 .

## Station relationships
metroRes:station/hospitalDeBellvitge metro:servesLine metroRes:metroLine/L1 ;
    metro:connectsTo metroRes:station/bellvitge .

metroRes:station/bellvitge metro:servesLine metroRes:metroLine/L1 ;
    metro:connectsTo metroRes:station/hospitalDeBellvitge ;
    metro:connectsTo metroRes:station/avCarrilet .

metroRes:station/avCarrilet metro:servesLine metroRes:metroLine/L1 ;
    metro:connectsTo metroRes:station/bellvitge .