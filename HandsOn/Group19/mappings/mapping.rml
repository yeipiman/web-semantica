@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix ex: <http://group19.linkeddata.es/resource/> .
@prefix ont: <http://group19.linkeddata.es/def/transport#> .

################################################################################
# TriplesMap: Aparcamientos p√∫blicos
################################################################################
<#TM_Parkings>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "202625-0-aparcamientos-publicos-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "http://group19.linkeddata.es/resource/parking/{id}" ;
    rr:class ont:ParkingFacility
  ] ;
  rr:predicateObjectMap [
    rr:predicate schema:name ;
    rr:objectMap [ rml:reference "name" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate schema:openingHours ;
    rr:objectMap [ rml:reference "openingHours" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate wgs:lat ;
    rr:objectMap [ rml:reference "latitud" ; rr:datatype xsd:float ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate wgs:long ;
    rr:objectMap [ rml:reference "longitude" ; rr:datatype xsd:float ]
  ] .

################################################################################
# TriplesMap: Estaciones BiciMAD
################################################################################
<#TM_BikeStations>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "bikestationbicimad_csv-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "http://group19.linkeddata.es/resource/bikestation/{id}" ;
    rr:class ont:BikeStation
  ] ;
  rr:predicateObjectMap [
    rr:predicate schema:name ;
    rr:objectMap [ rml:reference "name" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate schema:address ;
    rr:objectMap [ rml:reference "address" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate schema:capacity ;
    rr:objectMap [ rml:reference "totalDocks" ; rr:datatype xsd:integer ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:availableDocks ;
    rr:objectMap [ rml:reference "availableDocks" ; rr:datatype xsd:integer ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate wgs:lat ;
    rr:objectMap [ rml:reference "latitude" ; rr:datatype xsd:float ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate wgs:long ;
    rr:objectMap [ rml:reference "longitude" ; rr:datatype xsd:float ]
  ] .

################################################################################
# TriplesMap: Intensidad de movilidad mensual
################################################################################
<#TM_MobilityIntensity>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "I-M-D-LABORABLES-2024-2025-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "http://group19.linkeddata.es/resource/mobility/{year}/{month}/{zone}" ;
    rr:class ont:MobilityIntensity
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:year ;
    rr:objectMap [ rml:reference "year" ; rr:datatype xsd:integer ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:month ;
    rr:objectMap [ rml:reference "month" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:zone ;
    rr:objectMap [ rml:reference "zone" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ont:intensity ;
    rr:objectMap [ rml:reference "intensity" ; rr:datatype xsd:integer ]
  ] .
