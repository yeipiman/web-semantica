##################################################################
# TriplesMap: Episodios de Ozono
##################################################################
<#OzonoTriplesMap>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "csv/JUEGO-DATOS-activacion-protocolo-O3-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "http://example.org/resource/EpisodioOzono/{FECHA INICIO}" ;
    rr:class vocab:EpisodioOzono
  ] ;

  rr:predicateObjectMap [ rr:predicate vocab:fechaInicio ; rr:objectMap [ rml:reference "FECHA INICIO" ; rr:datatype xsd:dateTime ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:fechaFin ; rr:objectMap [ rml:reference "FECHA FIN" ; rr:datatype xsd:dateTime ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:episodio ; rr:objectMap [ rml:reference "EPISODIO OZONO" ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:medidaPoblacion ; rr:objectMap [ rml:reference "MEDIDA POBLACION" ] ] .