@prefix :    <http://example.org/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

###########
# SCHEMA  #
###########

# Classes
:Room         a rdfs:Class ;  rdfs:label "Room"@en , "Aula"@es .
:Sensor       a rdfs:Class ;  rdfs:label "Sensor"@en , "Sensor"@es .
:Measurement  a rdfs:Class ;  rdfs:label "Measurement"@en , "Medición"@es .
:Table        a rdfs:Class ;  rdfs:label "Table"@en , "Mesa"@es .
:Organization a rdfs:Class ;  rdfs:label "Organization"@en , "Organización"@es .

# Properties
:contains a rdf:Property ;
  rdfs:label "contains"@en , "contiene"@es ;
  rdfs:domain :Room ;
  rdfs:range  rdfs:Resource .

:hasMeasurement a rdf:Property ;
  rdfs:label "has measurement"@en , "tiene medición"@es ;
  rdfs:domain :Sensor ;
  rdfs:range  :Measurement .

:hasTemperature a rdf:Property ;
  rdfs:label "has temperature (ºC)"@en , "tiene temperatura (ºC)"@es ;
  rdfs:domain :Measurement ;
  rdfs:range  xsd:integer .

:atTime a rdf:Property ;
  rdfs:label "at time"@en , "en instante"@es ;
  rdfs:domain :Measurement ;
  rdfs:range  xsd:dateTime .

:hasOwner a rdf:Property ;
  rdfs:label "has owner"@en , "tiene propietario"@es ;
  rdfs:domain :Table ;
  rdfs:range  :Organization .

:hasName a rdf:Property ;
  rdfs:label "has name"@en , "tiene nombre"@es ;
  rdfs:domain :Organization ;
  rdfs:range  xsd:string .

###############
# INDIVIDUALS #
###############

:ETSIInformatics a :Organization ;
  rdfs:label "ETSI Informatics"@en ;
  :hasName "ETSI Informáticos" .

:Table322 a :Table ;
  rdfs:label "Table 322"@en ;
  :hasOwner :ETSIInformatics .

:Measurement8432 a :Measurement ;
  rdfs:label "Measurement 8432"@en ;
  :hasTemperature 29 ;
  :atTime "2022-09-12T12:01:12"^^xsd:dateTime .

:Sensor347 a :Sensor ;
  rdfs:label "Sensor 347"@en ;
  :hasMeasurement :Measurement8432 .

:ClassRoom03 a :Room ;
  rdfs:label "ClassRoom 03"@en , "Aula 03"@es ;
  :contains :Sensor347 , :Table322 .
